<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Background">#0d1116</Color>
    <Color x:Key="SecondaryBackground">#1d232a</Color>
    <Color x:Key="ComponentBackground">#161c23</Color>
    <Color x:Key="ComponentBorder">#161c23</Color>

    <Color x:Key="PrimaryForeground">#FFFFFF</Color>
    <Color x:Key="SecondaryForeground">#777f88</Color>
    <Color x:Key="ButtonForeground">#FFFFFF</Color>

    <Color x:Key="DiagramBackground">#0d1116</Color>
    <Color x:Key="DiagramPrimaryForeground">#FFFFFF</Color>
    <Color x:Key="DiagramWorkspaceBackground">#1d232a</Color>
    <Color x:Key="DiagramToolbarBackground">#1d232a</Color>
</ResourceDictionary>